#include <stdio.h>

int main() {
    int n, i, j, distinct = 1;
    scanf("%d", &n);
    int matrix[n][n];
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            if (matrix[i][i] == matrix[j][j]) {
                distinct = 0;
                break;
            }
        }
        if (!distinct) break;
    }
    if (distinct) {
        printf("True\n");
    } else {
        printf("False\n");
    }
    return 0;
}
